proc            /proc           proc    defaults          0       0
/dev/mmcblk0p1  /boot           vfat    defaults          0       2
/dev/mmcblk0p2  /               ext4    defaults,noatime,nodiratime,discard,ro  0       1
# a swapfile is not a swap partition, so no using swapon|off from here on, use  dphys-swapfile swap[on|off]  for that
none        /var/log        tmpfs   size=10M,noatime
none        /var/lib/dhcp        tmpfs   size=1M,noatime
none        /var/lib/misc        tmpfs   size=5M,noatime
